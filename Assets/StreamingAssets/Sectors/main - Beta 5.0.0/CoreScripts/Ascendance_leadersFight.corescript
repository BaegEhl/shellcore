Function(
    name=Ascendance_cs5_end,
    sequence=(
        FinishCameraPan(),
        Wait(time=1),
        SetPath(
            entityID=UninfectedLeader,
            flagName=Ascendance-cs5-flag7,
            rotateWhileMoving=false,
            customMass=1
        ),
        Wait(time=2),
        DeleteEntity(entityID=UninfectedLeader),
        DeleteEntity(entityID=weapon),
        FinishCutscene(),
        Call(Ascendance_leadersFightStart)
    )
)


